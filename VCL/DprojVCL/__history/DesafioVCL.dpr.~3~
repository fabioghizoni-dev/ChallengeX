program DesafioVCL;

uses
  Vcl.Forms,
  uMain in '..\UnitsVCL\uMainVCL\uMain.pas' {frmMain},
  uModule in '..\UnitsVCL\DataModuleVCL\uModule.pas' {DataModule1: TDataModule};

